{
  "name": "monash-taskable-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run protobuild || nuxt build",
    "dev": "pnpm run protobuild || nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "protobuild": "rm -f ./types/proto/*.ts && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./types ./proto/*"
  },
  "dependencies": {
    "@nuxt/icon": "^1.0.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@pinia/nuxt": "^0.5.1",
    "long": "^5.2.3",
    "nuxt": "^3.12.3",
    "pinia": "^2.1.7",
    "protobufjs": "^7.3.2",
    "sprintf-js": "^1.1.3",
    "vue": "latest"
  },
  "devDependencies": {
    "@iconify/icons-fluent": "^1.2.38",
    "@types/sprintf-js": "^1.1.4",
    "sass": "^1.77.6",
    "ts-proto": "^1.181.1",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "^4.2.2",
    "vue-svg-loader": "^0.16.0"
  }
}
